{
  "name": "oorja",
  "description": "cli tool for interacting with oorja",
  "version": "0.0.0",
  "author": "Akshay Kumar @akshaykmr",
  "bin": {
    "oorja": "./bin/run",
    "teletype": "./bin run teletype"
  },
  "bugs": "https://github.com/akshaykmr/oorja-cli/issues",
  "dependencies": {
    "@oclif/command": "^1",
    "@oclif/config": "^1",
    "@oclif/plugin-help": "^3",
    "chalk": "^4.1.0",
    "enquirer": "^2.3.6",
    "prettier": "^2.0.5",
    "tslib": "^1"
  },
  "devDependencies": {
    "@oclif/dev-cli": "^1",
    "@types/node": "^10",
    "globby": "^10",
    "ts-node": "^8",
    "tslint": "^6.1.1",
    "tslint-config-airbnb": "^5.11.2",
    "tslint-config-prettier": "^1.18.0",
    "tslint-plugin-prettier": "^2.3.0",
    "tslint-react": "^4.2.0",
    "typescript": "^3.3"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "files": [
    "/bin",
    "/lib",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json"
  ],
  "homepage": "https://github.com/akshaykmr/oorja-cli",
  "keywords": [
    "oorja",
    "cli"
  ],
  "license": "SEE LICENSE IN license.txt",
  "main": "lib/index.js",
  "oclif": {
    "commands": "./lib/commands",
    "bin": "oorja",
    "plugins": [
      "@oclif/plugin-help"
    ]
  },
  "repository": "akshaykmr/oorja-cli",
  "scripts": {
    "postpack": "rm -f oclif.manifest.json",
    "prepack": "rm -rf lib && tsc -b && oclif-dev manifest && oclif-dev readme",
    "test": "echo NO TESTS",
    "version": "oclif-dev readme && git add README.md",
    "lint": "tslint -p ./tsconfig.json -t stylish",
    "lint-fix": "tslint -p ./tsconfig.json -t stylish --fix",
    "prettier": "prettier --write './**/*.{ts,tsx,js,css,html}'"
  },
  "types": "lib/index.d.ts"
}
